[
    {
        "source": "compose.yml",
        "destination": "/etc/docker/abogutskiy.cloud",
        "action": "copy"
    },
    {
        "source": "abogutskiy.cloud.service",
        "destination": "/etc/systemd/system",
        "action": "copy",
        "permissions": "755"
    },
    {
        "source": "wireguard.tar.gz",
        "destination": "/etc/wireguard",
        "action": "unpack",
        "cleanup": true
    },
    {
        "source": "xray.tar.gz",
        "destination": "/etc/xray",
        "action": "unpack",
        "cleanup": true
    },
    {
        "source": "www.tar.gz",
        "destination": "/var/www",
        "action": "unpack",
        "cleanup": true
    },
    {
        "source": "cert.tar.gz",
        "destination": "/var/cert",
        "action": "unpack",
        "cleanup": true
    }
]
